{
    "name": "tenolo/doctrine-walker-bundle",
    "description": "This bundle add a event driven SQL output walker.",
    "keywords": [
        "symfony",
        "bundle",
        "doctrine",
        "sql",
        "walker",
        "tenolo"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "http://tenolo.de",
    "authors": [
        {
            "name": "tenolo GmbH & Co. KG",
            "email": "info@tenolo.de",
            "homepage": "http://tenolo.de"
        },
        {
            "name": "Nikita Loges",
            "email": "n.loges@tenolo.de"
        }
    ],
    "require": {
        "php": "^7.2",

        "symfony/framework-bundle": "^3.4|^4.0",
        "symfony/http-kernel": "^3.4|^4.0",
        "symfony/config": "^3.4|^4.0",
        "symfony/dependency-injection": "^3.4|^4.0",
        "symfony/doctrine-bridge": "^3.4|^4.0",
        "symfony/event-dispatcher": "^3.4|^4.0",
        "symfony/yaml": "^3.4|^4.0",

        "doctrine/orm": "^2.5",
        "doctrine/dbal": "^2.5",
        "doctrine/common": "^2.6",
        "doctrine/doctrine-bundle": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Tenolo\\Bundle\\DoctrineWalkerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tenolo\\Bundle\\DoctrineWalkerBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "scripts": {
        "cs": "phpcs --standard=PSR2 -n src",
        "cbf": "phpcbf --standard=PSR2 -n src",
        "unit": "phpunit --colors=always",
        "test": [
            "@unit",
            "@cs"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
